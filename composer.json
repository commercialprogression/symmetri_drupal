{
    "name": "commercialprogression/symmetri_drupal",
    "description": "Composer-based template for Symmetri Drupal 8 projects",
    "type": "project",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "Brad Czerniak",
            "role": "maintainer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "harvesthq/chosen",
                "version": "1.8.2",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/harvesthq/chosen/releases/download/v1.8.2/chosen_v1.8.2.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "^1.0"
                }
            }
        },
        {
            "type": "git",
            "url": "git@github.com:commercialprogression/compro_migrate_tools.git"
        }
    ],
    "require": {
        "commercialprogression/compro_migrate_tools": "dev-master",
        "composer/installers": "^1.0.20",
        "cweagans/composer-patches": "~1.0",
        "drupal-composer/drupal-scaffold": "^2.0.1",
        "drupal/core": "~8.0",
        "drush/drush": "~9.0",
        "drupal/console": "~1.0",
        "drupal/address": "~1.0",
        "drupal/admin_toolbar": "^1.23",
        "drupal/adminimal_theme": "^1.3",
        "drupal/adminimal_admin_toolbar": "^1.7",
        "drupal/advagg": "^3.4",
        "drupal/better_exposed_filters": "3.x-dev",
        "drupal/bg_image_formatter": "^1.1",
        "drupal/block_class": "^1.0@alpha",
        "drupal/chosen": "^2.5",
        "drupal/contact_storage": "^1.0@beta",
        "drupal/config_ignore": "^2.1",
        "drupal/config_split": "^1.3",
        "drupal/ctools": "^3.0",
        "drupal/devel": "^1.2",
        "drupal/diff": "^1.0@RC",
        "drupal/easy_breadcrumb": "^1.8",
        "drupal/eck": "^1.0@alpha",
        "drupal/entity_browser": "^2.0",
        "drupal/entity_reference_display": "^1.1",
        "drupal/features": "^3.5",
        "drupal/field_group": "3.x-dev",
        "drupal/google_analytics": "^2.2",
        "drupal/inline_entity_form": "^1.0@beta",
        "drupal/libraries": "^3.0@alpha",
        "drupal/linked_field": "^1.1",
        "drupal/linkit": "^4.3",
        "drupal/media_entity_browser": "^2.0",
        "drupal/menu_link_attributes": "^1.0",
        "drupal/menu_trail_by_path": "^1.1",
        "drupal/metatag": "^1.4",
        "drupal/migrate_tools": "4.x-dev",
        "drupal/migrate_plus": "4.x-dev",
        "drupal/path_redirect_import": "^1.0@beta",
        "drupal/pathauto": "^1.0",
        "drupal/recaptcha": "^2.2",
        "drupal/redirect": "^1.0",
        "drupal/search_api": "^1.8",
        "drupal/search_api_solr": "^1.2",
        "drupal/simple_sitemap": "^2.10",
        "drupal/smart_trim": "^1.1",
        "drupal/stage_file_proxy": "^1.0@alpha",
        "drupal/styleguide": "^1.0@alpha",
        "drupal/subpathauto": "^1.0@beta",
        "drupal/token": "^1.1",
        "drupal/video_embed_field": "2.x-dev",
        "drupal/viewsreference": "^1.2",
        "harvesthq/chosen": "^1.8.2"
    },
    "require-dev": {
        "behat/mink": "~1.7",
        "behat/mink-goutte-driver": "~1.2",
        "jcalderonzumba/gastonjs": "~1.0.2",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
        "mikey179/vfsStream": "~1.2",
        "phpunit/phpunit": "~4.8",
        "symfony/css-selector": "~2.8"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
      "installer-paths": {
        "web/core": [
          "type:drupal-core"
        ],
        "web/modules/contrib/{$name}": [
          "type:drupal-module"
        ],
        "web/profiles/contrib/{$name}": [
          "type:drupal-profile"
        ],
        "web/themes/contrib/{$name}": [
          "type:drupal-theme"
        ],
        "web/libraries/{$name}": [
          "type:drupal-library"
        ],
        "drush/contrib/{$name}": [
          "type:drupal-drush"
        ]
      }
    },
    "replace": {
        "drupal/drupal": "*"
    }
}
